<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asti3.palmbeachstatecollege">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".AdmissionActivity" />
        <activity android:name=".PantherwebActivity" />
        <activity android:name=".CatalogActivity" />
        <activity android:name=".BlackboardActivity" />
        <activity android:name=".EmailActivity" />
        <activity android:name=".AreaofStudyActivity" />
        <activity android:name=".LibraryActivity" />
        <activity android:name=".MapActivity" />
        <activity android:name=".DirectoryActivity" />
        <activity android:name=".TransportationActivity" />
        <activity android:name=".IncidentforStudentsActivity" />
        <activity android:name=".IncidentforEmployeeActivity" />
        <activity android:name=".LakeWorthTutorActivity" />
        <activity android:name=".PalmBeachGardentTutorActivity" />
        <activity android:name=".LoxTutorActivity" />
        <activity android:name=".BocaRatonTutorActivity" />
        <activity android:name=".BelleGladeActivity" />
        <activity android:name=".EventActivity" />
        <activity android:name=".NewsActivity" />
        <activity android:name=".YoutubeActivity" />
        <activity android:name=".FlickrActivity" />
        <activity android:name=".TwitterActivity" />
        <activity android:name=".FacebookActivity" />
        <activity android:name=".InstagramActivity" />
        <activity android:name=".DibsActivity" />

        <service android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name=".MFirebaseMessagingService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>